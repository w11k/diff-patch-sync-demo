{
  "name": "todo-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --live-reload false --proxy-config proxy.conf.json",
    "start:offline": "ng build && cd dist/todo-app/ && http-server -c-1 -p 4200 -a localhost --cors --proxy http://localhost:3000 .",
    "build": "ng build",
    "build:prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng build --prod --configuration=production",
    "test": "ng test",
    "test:e2e-ci": "(cd ../server && npm install && npm run start) & npm install && ./node_modules/protractor/bin/webdriver-manager update --versions.chrome=$(google-chrome --version | cut -d ' ' -f 3) && npm run e2e:headless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:withoutBuild": "ng e2e --devServerTarget=''",
    "e2e:headless": "ng e2e --configuration=headless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "^8.2.3",
    "@angular/forms": "^8.2.3",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.3",
    "@angular/platform-browser-dynamic": "^8.2.3",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^8.2.3",
    "@angular/service-worker": "~8.2.3",
    "@ngx-pwa/offline": "^6.2.0",
    "@types/uuid": "^3.4.6",
    "@w11k/diff-patch-sync": "0.0.16",
    "cache-manager": "^2.10.1",
    "chromedriver": "^80.0.0",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jsondiffpatch": "^0.4.1",
    "ng-connection-service": "^1.0.4",
    "ngx-indexed-db": "^3.0.15",
    "ngx-logger": "^4.0.8",
    "node-sass": "^4.13.1",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.5.3",
    "uuid": "^3.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.0"
  }
}
