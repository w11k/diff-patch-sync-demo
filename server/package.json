{
  "name": "todo-api",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run start:dev",
    "start:static": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "watch": "tsc -w",
    "serve": "node index.js",
    "migrate:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:run": "ts-node ./node_modules/typeorm/cli.js migration:run ",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert "
  },
  "dependencies": {
    "@anchan828/typeorm-decorators": "^0.3.33",
    "@hapi/joi": "^14.3.1",
    "@nestjs/common": "^6.8.3",
    "@nestjs/core": "^6.8.3",
    "@nestjs/platform-express": "^6.8.3",
    "@nestjs/platform-socket.io": "^6.8.3",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/testing": "^6.8.3",
    "@nestjs/websockets": "^6.8.3",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "@types/winston": "2.3.7",
    "@w11k/diff-patch-sync": "0.0.1",
    "automerge": "^0.12.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.10.2",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "date-fns": "^2.6.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.18.3",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "supertest": "^3.1.0",
    "swagger-ui-express": "^4.0.2",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typeorm": "^0.2.20",
    "typescript": "^3.0.1",
    "uuid": "^3.4.0",
    "winston": "2.4.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
